pipeline {
    agent any
    triggers {
        if (JOB_NAME.startsWith('EL/main') && BRANCH_NAME ~ '^(main|.*\\.el[0-9_]+)$') {
            cron ('* * * * *')
        }
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World!'
                echo JOB_NAME
                echo BRANCH_NAME
            }
        }
    }
}